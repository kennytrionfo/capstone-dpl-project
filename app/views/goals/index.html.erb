<p>Goals#index</p>
<br>
<br>


<table class="table">
  <thead>
    <tr>
      <th style="text-align:center">Catagory</th>
      <th style="text-align:center">Point Value</th>
      <th style="text-align:center">Frequency</th>
      <th style="text-align:center">Weekly Goal</th>
      <th style="text-align:center">Done!</th>
      <th style="text-align:center">Weekly Results</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goals.each do |goal| %>
      <tr style="text-align:center">
        <td><%= goal.category %></td>
        <td><%= goal.point_value %></td>
        <td><%= goal.frequency %></td>
        <td><%= "Weekly Goal here" %></td>
        <td><%= "Done button here" %></td>
        <td><%= "Weekly Results here" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
